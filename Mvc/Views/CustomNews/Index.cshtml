@model List<SitefinityWebApp.Mvc.Models.CustomNews>

<h1><------- Welcome to News -----></h1>

@foreach (var item in Model)
{
    <h3>Title: </h3>    @item.Title <h3> Author: </h3>@item.Author

    <h3>Description: </h3>@Html.Raw(item.Description)

    <h3>Image: </h3> <img src="@item.Image" width="100" height="100" />

}